# MedStation Environment Configuration Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# Development mode
MEDSTATION_ENV=development

# Admin Account
# Change these immediately on first startup
MEDSTATION_ADMIN_USERNAME=admin
MEDSTATION_ADMIN_PASSWORD=CHANGE_ME_IMMEDIATELY

# JWT Secret - Used to sign authentication tokens
# Generate with: openssl rand -hex 32
MEDSTATION_JWT_SECRET_KEY=GENERATE_ME_WITH_OPENSSL_RAND_HEX_32

# Optional: API Configuration
# MEDSTATION_API_PORT=8000

# Optional: Ollama Configuration
# OLLAMA_HOST=http://localhost:11434
